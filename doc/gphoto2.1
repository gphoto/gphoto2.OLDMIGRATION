.br
 [[\-u\ \fIFILENAME\fR] [\-\-upload\-file\ \fIFILENAME\fR]] [\-\-config]
.br
 [\-\-list\-config]
.br
 [\-\-get\-config\ \fICONFIGENTRY\fR]
.br
 [\-\-set\-config\ \fICONFIGENTRY=CONFIGVALUE\fR]
.br
 [\-\-capture\-preview]
